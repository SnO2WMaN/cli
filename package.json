{
	"name": "sno2wman",
	"version": "2.4.4",
	"description": "About @SnO2WMaN",
	"main": "dist/index.js",
	"author": "SnO2WMaN <sno2wman.official@gmail.com> (https://sno2wman.graphics/)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SnO2WMaN/SnO2WMaN.git"
	},
	"keywords": [
		"aboutme"
	],
	"engines": {
		"node": ">=8.0.0",
		"npm": ">=5.0.0"
	},
	"files": [
		"dist"
	],
	"lint-staged": {
		"*.js": [
			"eslint",
			"prettier",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn run lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"scripts": {
		"lint": "eslint **/*.js --ignore-path .gitignore",
		"flow": "flow",
		"build": "run-s build:*",
		"build:clean": "rimraf dist",
		"build:babel": "cross-env NODE_ENV=build babel src --out-dir dist",
		"test": "flow && ava",
		"minor": "npm version minor && git tag",
		"patch": "npm version patch && git tag",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-flow": "^7.0.0",
		"@commitlint/cli": "^7.5.1",
		"@commitlint/config-conventional": "^7.5.0",
		"@commitlint/travis-cli": "^7.5.1",
		"all-contributors-cli": "^6.0.0",
		"ava": "^1.2.1",
		"axios": "^0.18.0",
		"babel-eslint": "^10.0.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.13.0",
		"eslint-plugin-ava": "^5.1.1",
		"eslint-plugin-flowtype": "^3.2.1",
		"eslint-plugin-jsdoc": "^4.1.0",
		"flow-bin": "^0.93.0",
		"husky": "^1.3.1",
		"isobject": "^3.0.1",
		"lint-staged": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3"
	},
	"dependencies": {
		"ager": "^2.0.5",
		"gravatar": "^1.8.0"
	}
}
