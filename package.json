{
	"name": "sno2wman",
	"version": "2.4.5",
	"description": "About @SnO2WMaN",
	"author": "SnO2WMaN <sno2wman.official@gmail.com> (https://sno2wman.graphics/)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SnO2WMaN/SnO2WMaN.git"
	},
	"keywords": [
		"aboutme"
	],
	"engines": {
		"node": ">=8.0.0",
		"npm": ">=5.0.0"
	},
	"@pika/pack": {
		"pipeline": [
			[
				"@pika/plugin-standard-pkg",
				{
					"exclude": [
						"__tests__/*"
					]
				}
			],
			[
				"@pika/plugin-build-node"
			],
			[
				"@pika/plugin-build-web"
			],
			[
				"@pika/plugin-build-types"
			]
		]
	},
	"lint-staged": {
		"*.js": [
			"eslint",
			"prettier",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn run lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"scripts": {
		"lint": "linter-gi eslint && eslint . --fix --ext .js,.json,.md",
		"fmt": "linter-gi prettier && prettier **/*.{js,json,md} --write",
		"test": "ava",
		"build": "pack build"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@commitlint/cli": "^7.5.2",
		"@commitlint/config-conventional": "^7.5.0",
		"@pika/pack": "^0.3.4",
		"@pika/plugin-build-node": "^0.3.14",
		"@pika/plugin-build-types": "^0.3.14",
		"@pika/plugin-build-web": "^0.3.14",
		"@pika/plugin-standard-pkg": "^0.3.13",
		"all-contributors-cli": "^6.1.2",
		"ava": "^1.2.1",
		"axios": "^0.18.0",
		"babel-eslint": "^10.0.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.14.1",
		"eslint-config-cold": "^1.1.3",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"linter-gitignore": "^1.2.0",
		"prettier": "^1.16.4"
	},
	"dependencies": {
		"ager": "^2.0.5",
		"gravatar": "^1.8.0"
	}
}
