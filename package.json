{
	"name": "sno2wman",
	"version": "2.4.5",
	"description": "About @SnO2WMaN",
	"author": {
		"name": "SnO2WMaN",
		"email": "sno2wman.official@gmail.com"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SnO2WMaN/SnO2WMaN.git"
	},
	"main": "lib/index.js",
	"keywords": [
		"aboutme",
		"sno2wman"
	],
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*.{js,json}": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn run lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"scripts": {
		"lint": "eslint . --fix --ext .js,.json,.md",
		"fmt": "prettier **/*.{js,json,md} --write",
		"commit": "git-cz",
		"build": "npm-run-all build:*",
		"build:index": "rollup src/index.js --format cjs --file lib/index.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^7.5.2",
		"@commitlint/config-conventional": "^7.5.0",
		"all-contributors-cli": "^6.1.2",
		"commitizen": "^3.0.7",
		"eslint": "^5.15.3",
		"eslint-config-cold": "^1.1.5",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"rollup": "^1.7.3"
	},
	"dependencies": {
		"ager": "^2.0.5",
		"gravatar": "^1.8.0"
	}
}
